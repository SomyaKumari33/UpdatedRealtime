<core:FragmentDefinition
	xmlns:vh="sap.ui.comp.valuehelpdialog"
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<vh:ValueHelpDialog
        id="idResourceVHDia"
        key="resource"
        title="resource"
        supportRanges="false"
        supportMultiselect="false"
		ok=".onresourceVHDiaOkPress"
		cancel=".onresourceVHDiaCancelPress">
        <vh:filterBar>
            <fb:FilterBar id='idResourceVHDiaFB' search="onresourceVHDiaSearch">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem id="idresourceVHDiaFB-resourceFGI"
                        groupName="group1"
                        visibleInFilterBar="true"
                        name="resource"
                        label="Equipment">
                        <fb:control>
                            <Input id="idresourceVHDiaFB-resourceFGI-Input" 
                                placeholder="Search for resource..."/>
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </vh:filterBar>
	</vh:ValueHelpDialog>
</core:FragmentDefinition>